<div class="main-content">
  <div class="row">
    {{#each model as |dj|}}
      <div class="col-md-4">
        <h2>{{#link-to 'djs.dj' dj.username}}{{dj.username}}{{/link-to}}</h2>
        <img src={{dj.image_url}} />
        <p>
          {{dj.bio}}
        </p>
      </div>
    {{/each}}
  </div>
</div>
